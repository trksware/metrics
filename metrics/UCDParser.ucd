      className : String,
      ListAttsI : ArrayList<Item>,
      ListOpsI : ArrayList<Method>, 
      ListSubCla : ArrayList<ClassDec>,
      ListSupCla : ArrayList<ClassDec>,
      ListRela : ArrayList<String>,
      ListAgg : ArrayList<String>,
      ListAttsAll : ArrayList<String>,
      ListOpsAll : ArrayList<String>,
      depthU : Integer,
      ListSubClaNamesAll : ArrayList<String>,
      depthD : Integer,
      ListMetrics : ArrayList<String>,
      ANA : Double,
      NOM : Integer,
      NOA : Integer,
      ITC : Integer,
      ETC : Integer,
      CAC : Integer,
      DIT : Integer,
      CLD : Integer,
      NOC : Integer,
      NOD : Integer
OPERATIONS
      ClassDec(pStrDecl : String) : void,
      SetClassDec() : void,
      getClassName() : String,
      getStrDecl() : String,
      getListAttsI() : ArrayList<Item>,
      getListOpsI() : ArrayList<Method>,
      addSubClass(subClassName : ClassDec) : void,
      getListAttsNames() : ArrayList<String>,
      getListAttsNamesSTR() : String[],
      getListOpsNames() : ArrayList<String>,
