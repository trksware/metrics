MODEL UCDParser
CLASS GUI
ATTRIBUTES 
      text_path : Text,
      cont : Controler,
      classesNames : String[],
      text_details : Text,
      currentClassName : String
OPERATIONS
      main(args : String[]) : void
;

CLASS Controler
ATTRIBUTES 
      arrClasses : ArrayList<ClassDec>,
      types : List<String>,
      arrGeneralisations : ArrayList<GeneralisationD>,
      arrRelations : ArrayList<RelationD>,
      arrAggregations : ArrayList<AggregationD>,
      modelName : String,
      fileToREad : String
OPERATIONS
      Controler(pfileToREad : String) : void,
      getTypes() : List<String>,
      getArrClasses() : ArrayList<ClassDec>,
      getArrClassesNames() : ArrayList<String>,
      getClassesNames() : ArrayList<String>,
      getClassByName(className : String) : ArrayList<String>,
      getRealtionsNames() : ArrayList<String>,
      getClassesNamesTable() : String[],
      getStrDeclGen(currentClassName : String, subClass : String) : String,
      getStrDeclRel(relationName : String) : String,
      getStrDeclAgg(currentClassName : String,partName : String) : String,
      generateCSV() : void 
;

CLASS AggregationD
ATTRIBUTES 
      strDecl : String,
      container : String,
      parts : List<String>
OPERATIONS
      AggregationD(pStrDecl : String) : void,
      SetAggregationD() : void,
      getContainer() : String,
      getParts() : List<String>,
      getStrDecl() : String
;

CLASS GeneralisationD
ATTRIBUTES 
      strDecl : String,
      generalisationName : String,
      subclasses : List<String>
OPERATIONS
      GeneralisationD(pStrDecl : String) : void,
      SetGeneralisationD() : void,
      getGeneralisationName() : String,
      getStrDecl() : String,
      getSubclasses() : List<String>
;

CLASS RelationD
ATTRIBUTES 
      strDecl : String,
      relationName : String,
      firstClass : String,
      secondClass : String
OPERATIONS
      RelationD(pStrDecl : String) : void,
      SetRelationD() : void,
      getStrDecl() : String,
      getFirstClass() : String,
      getSecondClass() : String,
      getRelationName() : String
;

CLASS Method
ATTRIBUTES 
      name : String,
      typeReturn : String,
      ListP : List<Item>
OPERATIONS
      Method(pstring : String) : void,
      getName() : String,
      getTypeReturn() : String,
      getListP() : List<Item>
;

CLASS Item
ATTRIBUTES
      name : String,
      type : String,
      Ctype : Object
OPERATIONS
      Item(ptype : String, pname : String) : void,
      getName() : String,
      getType() : String,
      getCtype() : Object
;

