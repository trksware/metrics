RELATION est_sous_classe 
       ROLES
       CLASS ClassDec ONE_OR_MANY,
       CLASS ClassDec ONE
;

RELATION est_super_classe 
       ROLES
       CLASS ClassDec ONE_OR_MANY,
       CLASS ClassDec ONE
;

AGGREGATION 
CONTAINER 
       CLASS GUI ONE
PARTS
       CLASS Controler ONE
;

AGGREGATION 
CONTAINER 
       CLASS Controler ONE
PARTS
       CLASS AggregationD NONE_OR_MANY,
       CLASS RelationD NONE_OR_MANY,
       CLASS GeneralisationD NONE_OR_MANY,
       CLASS ClassDec NONE_OR_MANY
;

AGGREGATION 
CONTAINER 
       CLASS ClassDec ONE
PARTS
       CLASS Method NONE_OR_MANY,
       CLASS Item NONE_OR_MANY
;

AGGREGATION 
CONTAINER 
       CLASS Method ONE
PARTS
       CLASS Item NONE_OR_MANY
;